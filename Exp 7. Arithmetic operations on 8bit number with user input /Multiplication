global _start
section .data
msg1 db "Enter first number ", 10
len1 equ $ -msg1
msg2 db "Enter second number ", 10
len2 equ $ -msg2
a db 0, 0
b db 0, 0
res db 0, 0
section .text
_start:
mov eax, 4
mov ebx, 1
mov ecx, msg1
mov edx, len1
int 80h
mov eax, 3
mov ebx, 0
mov ecx, a
mov edx, 2
int 80h
mov eax, 4
mov ebx, 1
mov ecx, msg2
mov edx, len2
int 80h
mov eax, 3
mov ebx, 0
mov ecx, b
mov edx, 2
int 80h
mov al, [a]
sub al, '0'
mov bl, [b]
sub bl, '0'
mul bl
add al, '0'
mov [res], al
mov eax, 4
mov ebx, 1
mov ecx, res
mov edx, 2
int 80h
mov eax, 1
xor ebx, ebx
int 80h
