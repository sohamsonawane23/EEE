global _start
section .data
msg1 db "Enter Name ", 10
len1 equ $ -msg1
msg2 db "Enter roll no and address ", 10
len2 equ $ -msg2
section .bss
a resb 20
b resb 20
section .text
_start:
mov eax, 4
mov ebx, 1
mov ecx, msg1
mov edx, len1
int 80h
mov eax, 3
mov ebx, 0
mov ecx, a
mov edx, 20
int 80h
mov eax, 4
mov ebx, 1
mov ecx, msg2
mov edx, len2
int 80h
mov eax, 4
mov ebx, 1
mov ecx, msg2
mov edx, len2
int 80h
mov eax, 4
mov ebx, 1
mov ecx, a
mov edx, 20
int 80h
mov eax, 4
mov ebx ,1
mov ecx, b
mov edx, 20
int 80h
mov eax, 1
xor ebx, ebx
int 80h
